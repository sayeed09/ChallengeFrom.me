      ------------------Appp.js-----------------









      --------------------Response Data------

      import React from 'react';
import data from './responsedata';
import './dare.css';

class ResponseDare extends React.Component {
    constructor(props) {
        super(props);
        this.state = {
            classNames: ['', '', '', ''],
            nr:'0',
            AnswerArray: new Array(10),
        }
        this.checkAnswer = this.checkAnswer.bind(this);

    }
  
    checkAnswer(){

    }

    pushData(nr) {
        var that=this;
        var i=0;
        for(i=0;i<=data[nr].options.length;i++){
            var newStateArray = this.state.AnswerArray.slice();
            newStateArray.push(data[nr].options[i]);
                this.setState({myArray: newStateArray});
            //this.setState({ answers: this.state.answers.concat(data[nr].options[i].option_value) })
          // this.state.answers.push(data[nr].options[i].option_value)
        
        }
        this.setState({
            question: data[nr].question,
            correct: data[nr].answer_id,
            nr: this.state.nr + 1
        });
    }

    componentWillMount() {
        let { nr } = this.state;
        this.pushData(nr);
    }
    shouldComponentUpdate() {
        this.setState({
            classNames: ['', '', '', '']
        });
        return true;
    }
    
    render() {
     
        let { classNames } = this.state;
        
        let transition = {
            transitionName: "example",
            transitionEnterTimeout: 500,
            transitionLeaveTimeout: 300
        }
        
        return (
            <div id="answers">
            <p>{this.state.question}</p>
                <ul>
                    <li onClick={this.checkAnswer} className={classNames[0]} data-id="1"><span>A</span> <p>{this.state.answers[0]}</p></li>
                    <li onClick={this.checkAnswer} className={classNames[1]} data-id="2"><span>B</span> <p>{this.state.answers[1]}</p></li>
                    <li  onClick={this.checkAnswer} className={classNames[2]} data-id="3"><span>C</span> <p>{this.state.answers[2]}</p></li>
                    <li onClick={this.checkAnswer}  className={classNames[3]} data-id="4"><span>D</span> <p>{this.state.answers[3]}</p></li>
                   {(!this.state.answers[4]!=='') && <li onClick={this.checkAnswer}  className={classNames[3]} data-id="5"><span>E</span> <p>{this.state.answers[4]}</p></li>
                   }
                </ul>
            </div>
        );
    }
}

export default ResponseDare;
